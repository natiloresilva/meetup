<div class="card card-meeting">
    {{#if this.meetingImg}}
        <img class="card-img-top" src="{{this.meetingImg}}" alt="Meeting picture">
    {{^}}
        <img src="../../images/bar.jpg" class="card-img-top" alt="bar">
    {{/if}}
    <div class="card-body">
        <div id="background-title">
            <h5 class="card-title">{{this.meetingName}}</h5>
        </div>
        <p class="card-text">{{this.meetingLanguage}}</p>
        <p class="card-text">{{this.meetingCity}}</p>
        <a href="/meet/meetings/{{this._id}}" class="btn btn-primary">More Details!</a>
    </div>
</div>